.appointment{
    padding: 158rpx 39rpx;
}
.appointment .item{
    position: relative;
    padding: 40rpx;
    border: 4rpx;
    background: #f2f5fb;
    margin-bottom: 40rpx;
}
.appointmentTitle{
    display: inline-block;
    font-size: 24rpx;
    color: #777;
    width: 172rpx;
}
.appointmentB{
    font-size: 24rpx;
    color: #555;
    margin-right: 16rpx;
}
.appointment .item image{
    width: 128rpx;
    height: 128rpx;
    position: absolute;
    top: 81rpx;
    right: 40rpx;
}

.createClass{
    width: 88rpx;
    height: 88rpx;
    position: absolute;
    bottom: 40rpx;
    right: 40rpx;
}

.modal{
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.2);
    position: absolute;
    top: 0rpx;
    left: 0rpx;
}
.modal .contant{
    width: 540rpx;
    height: 493rpx;
    background: #fff;
    border-radius: 4rpx;
    box-shadow: 10px rgba(0, 0, 0, 0.2);
    margin: 300rpx 120rpx;
}
.modal .top{
    padding: 40rpx 40rpx 0rpx 40rpx;
    font-size: 24rpx;
    line-height: 60rpx;
    height: 387rpx;
    border-bottom: 1px solid #dcdcdc;
}

.modal .top input{
    height: 60rpx;
    width: 324rpx;
    color: #777;
    border: 1px solid #dcdcdc;
    margin-left: 10rpx;
    padding-left: 24rpx;
}
.modal .top view{
    margin-top: 24rpx;
}
.modal .top view text{
    color: #555;
}
.modal .bottom{
    height: 80rpx;
}
.modal .bottom view{
    height: 80rpx;
    width: 269rpx;
    background: #fff;
    font-size: 24rpx;
    color: #4b94fa;
    line-height: 80rpx;
    text-align: center;
}
.modal .bottom .red{
    width: 270rpx;
    color: #fe4f5c;
    border-left: 1px solid #dcdcdc;
}