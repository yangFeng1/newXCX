page{
    background: #e6eaf3;
    height: 100%;
}
.mybody{
  height: 100%;
}
.tabar view{
    height: 98rpx;
    width: 250rpx;
    position: relative;
    background: #63a0fa;
}
.tabar view text{
    height: 98rpx;
    width: 250rpx;
    line-height: 98rpx;
    text-align: center;
    font-size: 28rpx;
    color: #fff;
    position: absolute;
    z-index: 999;
}
.tabar view image{
    height: 98rpx;
    width: 250rpx;
    position: absolute;
    top: 0rpx;
    left: 0rpx;
}
.startClass{
    /* position: relative; */
}
.startClass input{
    height: 80rpx;
    padding-left: 40rpx;
    background: #d8dce5;
    font-size: 24rpx;
    color: #999;
    line-height: 80rpx;
}
.startClass input:focus {
    background: #fff;
}
.startClass .item{
    height: 104rpx;
    padding: 0 40rpx;
    background: #f2f5fb;
    font-size: 26rpx;
    color: #555;
    line-height: 104rpx;
    border: 1px solid #f2f5fb;
}
.startClass .selected{
    background: #d7e4fc;
    border: 1px solid #ced1d9;
}
.tabar .hide{
    position: absolute;
    bottom: -97rpx;
    z-index: 99999;
}
.tabar .hide view{
    width: 250rpx;
    height: 98rpx;
    background: #69a8ff;
    font-size: 28rpx;
    color: #fff;
    line-height: 98rpx;
    text-align: center;
}
.tabar .hide .beginClass{
    background: #4b94fa;
}
.black{
    color: #555;
}
.gray{
    color: #999;
}
.call{
    position: absolute;
    bottom:0rpx;
    width: 100%;
    height: 96rpx;
    background: #4b94fa;
    font-size: 30rpx;
    color: #fff;
    line-height: 98rpx;
    text-align: center;
}

.insert{
    padding: 218rpx 100rpx 0rpx 100rpx;
}
